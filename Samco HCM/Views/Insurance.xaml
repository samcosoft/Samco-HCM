<dxwui:NavigationPage x:Class="Samco_HCM.Views.Insurance"
                      xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                      xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                      xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
                      xmlns:d="http://schemas.microsoft.com/expression/blend/2008" 
                      xmlns:dxwui="http://schemas.devexpress.com/winfx/2008/xaml/windowsui"
                      xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol"
                      mc:Ignorable="d" FlowDirection="RightToLeft"
                      d:DesignHeight="450" d:DesignWidth="800">
    <dxwui:PageAdornerControl Header="خدمات اورژانس">
        <dxlc:TileLayoutControl Name="MainTilLayout" AllowAddFlowBreaksDuringItemMoving="False" Background="#660066">
            <dxlc:TileLayoutControl.Resources>
                <Style TargetType="TextBlock">
                    <Setter Property="FontFamily" Value="{StaticResource Vazir}"/>
                </Style>
            </dxlc:TileLayoutControl.Resources>
        </dxlc:TileLayoutControl>
    </dxwui:PageAdornerControl>
</dxwui:NavigationPage>
