<UserControl x:Class="Samco_HCM_Laboratory_Client.Views.LabResultViews.TestNameView"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006" 
             xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
             xmlns:dxlc="http://schemas.devexpress.com/winfx/2008/xaml/layoutcontrol"
             xmlns:labResultViews="clr-namespace:Samco_HCM_Laboratory_Client.Views.LabResultViews"
             mc:Ignorable="d" 
             d:DesignHeight="450" d:DesignWidth="800" d:DataContext="{d:DesignInstance labResultViews:TestGroupData}">

    <dxlc:LayoutGroup Name="LabTestList" Header="{Binding GroupName}" View="GroupBox" Orientation="Vertical" MinWidth="200" ItemSpace="10">
       
    </dxlc:LayoutGroup>
</UserControl>
